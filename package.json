{
	"name": "talakutnangan",
	"description": "A capstone project aim to create a consultation platform through chat for MCC",
	"version": "0.9.0-dev",
	"homepage": "https://github.com/lemredd/talakutnangan",
	"keywords": [
		"front-end",
		"web",
		"chat"
	],
	"private": true,
	"types": "module",
	"scripts": {
		"prod": "npm run build && npm run server:prod",
		"build": "vite build && vite build --ssr",
		"server:prod": "cross-env NODE_ENV=production ts-node ./server",
		"changelog:generate": "npx changelogen",
		"migrate:initial": "sequelize-cli db:create && npm run migrate:up",
		"migrate:reset": "sequelize-cli db:drop && npm run migrate:initial",
		"migrate:up": "sequelize-cli db:migrate",
		"migrate:down": "sequelize-cli db:migrate:undo",
		"migrate:redo:all": "sequelize-cli db:drop && sequelize-cli db:create && npm run migrate:up",
		"seed:make": "npx sequelize-cli seed:generate --name",
		"seed:up:all": "npx sequelize-cli db:seed:all",
		"seed:down": "npx sequelize-cli db:seed:undo",
		"seed:redo:all": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all"
	},

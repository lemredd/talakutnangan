{
	"name": "talakutnangan",
	"description": "A capstone project aim to create a consultation platform through chat for MCC",
	"version": "0.4.0-dev",
	"homepage": "https://github.com/lemredd/talakutnangan",
	"keywords": [
		"front-end",
		"web",
		"chat"
	],
	"private": true,
	"types": "module",
	"scripts": {
		"dev": "nodemon --watch server --ext ts --ignore \"*.spec.ts\" --exec \"npm run server\"",
		"list:routes": "ts-node ./server/cli/list_routes.ts",
		"prod": "npm run build && npm run server:prod",
		"build": "vite build && vite build --ssr",
		"server": "ts-node ./server",
		"server:prod": "cross-env NODE_ENV=production ts-node ./server",
		"changelog:generate": "npx changelogen",
		"migrate:initial": "sequelize-cli db:create && npm run migrate:up",
		"migrate:reset": "sequelize-cli db:drop && npm run migrate:initial",
		"migrate:up": "sequelize-cli db:migrate",
		"migrate:down": "sequelize-cli db:migrate:undo",
		"migrate:redo:all": "sequelize-cli db:drop && sequelize-cli db:create && npm run migrate:up",
		"test:unit:front": "cross-env NODE_ENV=unit_test jest -c jest.front.config.json",
		"test:unit:back": "cross-env NODE_ENV=unit_test jest -c jest.back.config.json",
		"test:intg:back": "cross-env NODE_ENV=intg_test jest -c jest.back.intg.config.json --detectOpenHandles",
		"test:unit:front:w": "npm run test:unit:front -- --watch",
		"test:unit:back:w": "npm run test:unit:back -- --watch",
		"test:intg:back:w": "npm run test:intg:back -- --watch"
	},
